{
  "name": "lb-hounslow/hounslow-api-client",
  "description": "An interface for the Hounslow API",
  "type": "library",
  "license": "proprietary",
  "require": {
    "php": ">=7.4.2",
    "ext-json": "*",
    "guzzlehttp/guzzle": "^7.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  },
  "config": {
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "LBHounslow\\ApiClient\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Unit\\": "tests/unit/"
    }
  }
}
